{
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "interface-name": [
        true,
        "never-prefix"
      ],
      "no-implicit-dependencies": [
        true,
        "dev"
      ],
      "no-var-keyword": true,
      "typedef": [
        true,
        "call-signature"
      ],
      "max-classes-per-file": false,
      "max-line-length": false,
      "no-console": false,
      "no-submodule-imports": false,
      "object-literal-sort-keys": false,
      "indent": [
        true,
        "spaces",
        4
      ]
    }
  }